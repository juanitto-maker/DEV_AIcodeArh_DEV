  },
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {},
  "dependencies": {},
  "files": [
    "index.html",
    "core.js",
    "ai.js",
    "ui.js",
    "styles.css",
    "vercel.json",
    "README.md"
  ],
  "browserslist": [
    "> 1%",
